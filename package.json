{
  "name": "@midwayjs/grpc-helper",
  "version": "1.0.3",
  "description": "Midway gRPC helper",
  "main": "dist/index",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "midway-bin build -c && rm -rf ./dist/.mwcc-cache",
    "test": "midway-bin test --ts",
    "cov": "midway-bin cov --ts",
    "ci": "npm run test",
    "lint": "mwts lint",
    "lint:fix": "mwts fix"
  },
  "bin": {
    "tsproto": "bin/tsproto.js"
  },
  "keywords": [
    "midway",
    "gRPC",
    "proto",
    "generator"
  ],
  "files": [
    "bin",
    "template.hbs",
    "dist/**/*.js",
    "dist/**/*.d.ts"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/node": "^14.0.11",
    "@midwayjs/cli": "^1.0.0",
    "mwts": "^1.0.5"
  },
  "dependencies": {
    "chalk": "^4.0.0",
    "fs-extra": "^9.0.1",
    "handlebars": "^4.7.6",
    "protobufjs": "^6.9.0",
    "yargs": "^15.3.1"
  },
  "author": "Harry Chen <czy88840616@gmail.com>",
  "repository": {
    "type": "git",
    "url": "http://github.com/midwayjs/grpc-helper.git"
  }
}
